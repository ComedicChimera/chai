package e03_control_flow;

import println, scanf from io.std;

// fibonacci calculates the nth Fibonacci number where n is inputted by the user.
func fibonacci() {
    let n: i64;
    scanf("%d", &n);

    let a = 0, b = 1;
    for let i = 0; i < n; i++ {
        a, b = b, a + b;
    }

    println(a);
}