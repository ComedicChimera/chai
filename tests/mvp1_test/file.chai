import print_byte from core.sys

def print_digit(d: i32) =
        print_byte('0' + (d as rune) as u8)
    
def print_int(i: i32) = do
    let x = i
    let p = 10

    while p < i do
        print_digit(x % 10)
        x //= 10
        p *= 10
    end
end

def square(x: i32) i32 = x * x

def main() = do
    let s = square(12)
    print_int(s)
end